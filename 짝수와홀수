class Solution {
    public String solution(int num) {
        String answer = "";
        if(num%2==1){
            answer = "Odd";
            System.out.println(answer);
        }
        
        else if(num%2==-1){
            answer = "Odd";
            System.out.println(answer);
        }
        
        else{
            answer= "Even";
            System.out.println(answer);
        }
